{
  "name": "dsm23-bun-svelte-kit-template",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint:js": "eslint \"**/*.{js,mjs,ts,tsx,svelte}\" --report-unused-disable-directives --max-warnings 0",
    "lint:css": "stylelint \"**/*.css\" --max-warnings 0 --allow-empty-input --cache --cache-strategy=content --cache-location=.stylelintcache",
    "lint": "run-p lint:js lint:css",
    "test:unit": "vitest --project=client --project=server",
    "test": "vitest",
    "test:e2e": "playwright test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.10.2",
    "@chromatic-com/storybook": "4.1.0",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@commitlint/types": "19.8.1",
    "@eslint/compat": "1.3.2",
    "@eslint/js": "9.33.0",
    "@ianvs/prettier-plugin-sort-imports": "4.6.1",
    "@playwright/test": "1.54.2",
    "@storybook/addon-a11y": "9.1.2",
    "@storybook/addon-docs": "9.1.2",
    "@storybook/addon-svelte-csf": "5.0.7",
    "@storybook/addon-vitest": "9.1.2",
    "@storybook/sveltekit": "9.1.2",
    "@sveltejs/adapter-node": "5.2.14",
    "@sveltejs/kit": "2.27.3",
    "@sveltejs/vite-plugin-svelte": "6.1.1",
    "@tailwindcss/vite": "4.1.11",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/node": "24.2.1",
    "@vitest/browser": "3.2.4",
    "dotenv": "17.2.1",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-storybook": "9.1.2",
    "eslint-plugin-svelte": "3.11.0",
    "globals": "16.3.0",
    "lefthook": "1.12.2",
    "npm-run-all2": "8.0.4",
    "playwright": "1.54.2",
    "prettier": "3.6.2",
    "prettier-plugin-css-order": "2.1.2",
    "prettier-plugin-svelte": "3.4.0",
    "prettier-plugin-tailwindcss": "0.6.14",
    "storybook": "9.1.2",
    "stylelint": "16.23.1",
    "stylelint-config-standard": "39.0.0",
    "stylelint-order": "7.0.0",
    "stylelint-selector-bem-pattern": "4.0.1",
    "svelte": "5.38.0",
    "svelte-check": "4.3.1",
    "tailwindcss": "4.1.11",
    "typed-query-selector": "2.12.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.39.1",
    "vite": "7.1.1",
    "vitest": "3.2.4",
    "vitest-browser-svelte": "1.1.0"
  }
}
